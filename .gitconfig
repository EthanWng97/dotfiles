[core]
    pager = delta

[interactive]
    diffFilter = delta --color-only
[delta]
    navigate = true  # use n and N to move between diff sections
	line-numbers = true
	syntax-theme = "Dracula"



[merge]
    conflictstyle = diff3

[diff]
    colorMoved = default
